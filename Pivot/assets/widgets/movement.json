{
    "variables": {
        "buttonSize": ["data", "size"],
        "buttonScale": ["data", "scale"],
        "buttonDirection": ["children", "up", "data", "texture"],
        "buttonPushed": ["children", "down", "data", "texture"],
        "buttonAnchor": ["data", "anchor"],
        "buttonHorizontal": ["layout", "x_anchor"],
        "buttonXPos": ["layout", "x_offset"],
        "buttonYPos": ["layout", "y_offset"],
        "dash": ["children", "dash", "data", "visible"],
        "dashAppearance": ["children", "dash", "data", "texture"],
        "dashAnchor": ["children", "dash", "data", "anchor"],
        "dashHorizontal": ["children", "dash", "layout", "x_anchor"],
        "dashXPosition": ["children", "dash", "layout", "x_offset"],
        "dashActive": ["children", "dashactive", "data", "visible"],
        "dashActiveAppearance": ["children", "dashactive", "data", "texture"],
        "dashActiveAnchor": ["children", "dashactive", "data", "anchor"],
        "dashActiveHorizontal": ["children", "dashactive", "layout", "x_anchor"],
        "dashActiveXPosition": ["children", "dashactive", "layout", "x_offset"]
    },
    "contents": {
        "type": "Button",
        "format": {
            "type": "Anchored"
        },
        "data": {
           "upnode": "up",
           "downnode": "down",
           "size": [200,200],
           "anchor": [0,0],
           "scale": 1
        },
        "children": {
            "up":{
                "type": "Image",
                "data": {
                    "texture": "left"
                }
            },
            "down":{
                "type": "Image",
                "data": {
                    "texture": "leftactive"
                }
            },
            "dashactive": {
                "type": "Image",
                "data": {
                    "texture": "leftdashactive",
                    "visible": true,
                    "size": [95,110],
                    "anchor": [1,0.5]
                },
                "layout": {
                    "x_anchor": "left",
                    "y_anchor": "middle",
                    "x_offset": 0.1
                }
            },
            "dash": {
                "type": "Image",
                "data": {
                    "texture": "leftdash",
                    "visible": true,
                    "size": [95,110],
                    "anchor": [1,0.5]
                },
                "layout": {
                    "x_anchor": "left",
                    "y_anchor": "middle",
                    "x_offset": 0.1
                }
            }
        },
        "layout": {
            "x_anchor": "left",
            "y_anchor" : "bottom",
            "x_offset" : 0.05,
            "y_offset" : 0.05
        }
    }
}

