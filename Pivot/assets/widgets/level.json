{
    "variables": {
        "levelAnchor": ["data", "anchor"],
        "showLevel": ["data", "visible"],
        "levelStartX" : ["layout", "x_anchor"],
        "levelStartY" : ["layout", "y_anchor"],
        "levelPosX" : ["layout", "x_offset"],
        "levelPosY" : ["layout", "y_offset"],
        "levelState": ["children", "up", "data", "texture"],
        "levelNumber": ["children", "label", "data", "text"]
    },
    "contents": {
        "type": "Button",
        "format": {
            "type": "Anchored"
        },
        "data": {
            "upnode": "up",
            "size": [300,300],
            "anchor": [0, 1],
            "scale": 1,
            "visible": true
        },
        "layout":{
            "x_anchor": "left",
            "y_anchor": "top",
            "x_offset": 0,
            "y_offset": 0
        },
        "children": {
            "up":{
                "type": "Image",
                "data": {
                    "texture": "unlocked"
                }
            },
            "label": {
                "type": "Label",
                "data": {
                    "font": "roboto",
                    "text": "1",
                    "foreground": [255,255,255,255],
                    "background": [0,0,0,0],
                    "halign": "center",
                    "valign": "middle"
                },
                "layout":{
                    "x_anchor": "fill",
                    "y_anchor": "fill"
                }
            }
        }
    }
}
